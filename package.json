    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest tests/integration",
    "test:auth": "vitest tests/integration/auth.test.js",
    "test:auth-unit": "vitest tests/unit/auth.test.js",
    "test:auth-watch": "vitest tests/integration/auth.test.js --watch",
    "test:tasks": "vitest tests/integration/tasks.test.js",
    "test:tasks-unit": "vitest tests/unit/tasks.test.js",
    "test:tasks-watch": "vitest tests/integration/tasks.test.js --watch",
    "test:run": "vitest run",
    "test:setup": "node  --no-deprecation tests/setup/manualSetup.js",
    "test:ui": "vitest --ui",
    "test:connection": "node --no-deprecation tests/setup/testConnection.js",
    "test:seed": "node --no-deprecation tests/setup/seedOnly.js"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "chart.js": "^4.5.0",
